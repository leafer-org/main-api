          "401": {
            "description": "Не авторизован",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "404": {
            "description": "Файл не найден",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/banners": {
      "get": {
        "operationId": "getBanners",
        "tags": ["Banners"],
        "description": "Get all banners",
        "parameters": [
          {
            "name": "filter",
            "in": "query",
            "required": false,
            "schema": {
              "type": "object",
              "properties": {
                "createdAt": {
                  "$ref": "#/components/schemas/DateRange"
                }
              }
            }
          },
          {
            "name": "pagination",
            "in": "query",
            "required": false,
            "schema": {
              "$ref": "#/components/schemas/Pagination"
            }
          },
          {
            "name": "sort",
            "in": "query",
            "required": false,
            "schema": {
              "$ref": "#/components/schemas/Sort"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "List of banners",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "required": ["edges", "pageInfo", "totalCount"],
                  "properties": {
                    "edges": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/BannerEdge"
                      }
                    },
                    "pageInfo": {
                      "$ref": "#/components/schemas/PageInfo"
                    },
                    "totalCount": {
                      "type": "integer",
                      "minimum": 0
                    }
                  }
                }
              }
            }
          },
          "404": {
            "$ref": "#/components/responses/NotFoundError"
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    },
    "schemas": {
      "ThrottledErrorResponse": {
        "type": "object",
        "required": ["code", "retryAfterSec"],
        "properties": {
          "code": {
            "type": "string",
            "enum": ["throttled"]
          },
          "message": {
            "type": "string"
          },
          "retryAfterSec": {
            "type": "number",
            "minimum": 1,
            "description": "Рекомендованная задержка перед повторной попыткой"
          }
        }
      },
      "ErrorResponse": {
        "type": "object",
        "required": ["code"],
        "properties": {
          "code": {
            "type": "string",
            "description": "Код ошибки для обработки на клиенте"
          },
          "message": {
            "type": "string",
            "description": "Человекочитаемое описание ошибки"
          },
          "details": {
            "type": "object",
            "additionalProperties": true,
            "description": "Дополнительные детали ошибки"
          }
        }
      },
      "TokenPair": {
        "type": "object",
        "required": ["accessToken", "refreshToken"],
        "properties": {
          "accessToken": {
            "type": "string",
            "minLength": 16,
            "description": "JWT access-токен"
          },
          "refreshToken": {
            "type": "string",
            "minLength": 16,
            "description": "JWT refresh-токен"
          }
        }
      },
      "MediaVisibility": {
        "type": "string",
        "enum": ["PUBLIC", "PRIVATE"]
      },
      "LinkMediaData": {
        "type": "object",
        "required": ["bucket", "objectKey", "mediaId", "visibility"],
        "properties": {
          "bucket": {
            "type": "string"
          },
          "objectKey": {
            "type": "string"
          },
          "mediaId": {
            "type": "string",
            "minLength": 1,
            "description": "Идентификатор загруженного медиа-объекта"
          },
          "visibility": {
