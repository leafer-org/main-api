{
  "openapi": "3.1.0",
  "info": {
    "version": "1.0.0",
    "title": "Playerok OpenAPI Schema",
    "description": "Playerok OpenAPI Schema"
  },
  "servers": [
    {
      "url": "/"
    }
  ],
  "paths": {
    "/auth/request-otp": {
      "post": {
        "operationId": "requestOtp",
        "tags": ["auth"],
        "summary": "Запрос одноразового кода подтверждения",
        "description": "Создаёт и отправляет OTP пользователю.",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["phoneNumber"],
                "additionalProperties": false,
                "properties": {
                  "phoneNumber": {
                    "type": "string",
                    "pattern": "^\\+?[1-9]\\d{9,14}$",
                    "description": "Телефон в формате E.164"
                  },
                  "channel": {
                    "type": "string",
                    "enum": ["sms", "whatsapp"],
                    "description": "Канал доставки OTP"
                  },
                  "locale": {
                    "type": "string",
                    "minLength": 2,
                    "maxLength": 5,
                    "description": "Локаль для текстов уведомлений"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OTP отправлен",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "additionalProperties": false,
                  "properties": {
                    "nextRetrySec": {
                      "type": "number",
                      "minimum": 1,
                      "description": "Рекомендованная задержка до следующего запроса OTP"
                    }
                  }
                }
              }
            }
          },
          "429": {
            "description": "Слишком много запросов",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ThrottledErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/auth/verify-otp": {
      "post": {
        "operationId": "verifyOtp",
        "tags": ["auth"],
        "summary": "Подтверждение одноразового кода",
        "description": "Проверяет OTP и возвращает пару токенов.",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["phoneNumber", "code"],
                "additionalProperties": false,
                "properties": {
                  "phoneNumber": {
                    "type": "string",
                    "pattern": "^\\+?[1-9]\\d{9,14}$",
                    "description": "Телефон в формате E.164"
                  },
                  "code": {
                    "type": "string",
                    "pattern": "^\\d{4,6}$",
                    "description": "Код подтверждения из SMS"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Авторизация успешна",
            "content": {
              "application/json": {
                "schema": {
                  "discriminator": {
                    "propertyName": "type"
                  },
                  "oneOf": [
                    {
                      "type": "object",
                      "required": ["type", "accessToken", "refreshToken"],
                      "additionalProperties": false,
                      "properties": {
                        "type": {
                          "type": "string",
                          "enum": ["authenticated"]
                        },
                        "accessToken": {
                          "type": "string",
                          "minLength": 16,
                          "description": "JWT access-токен"
                        },
                        "refreshToken": {
                          "type": "string",
                          "minLength": 16,
                          "description": "JWT refresh-токен"
                        }
                      }
                    },
                    {
                      "type": "object",
                      "required": ["type", "registrationSessionId"],
                      "additionalProperties": false,
                      "properties": {
                        "type": {
                          "type": "string",
                          "enum": ["new_registration"]
                        },
                        "registrationSessionId": {
                          "type": "string",
                          "description": "Идентификатор сессии регистрации"
                        }
                      }
                    }
                  ]
                }
              }
            }
          },
          "400": {
            "description": "Некорректный запрос",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "403": {
            "description": "Превышено количество попыток",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "required": ["code"],
                  "additionalProperties": false,
                  "properties": {
                    "code": {
                      "type": "string",
                      "enum": ["otp_attempts_exceeded"]
                    },
                    "message": {
                      "type": "string"
                    },
                    "retryAfterSec": {
                      "type": "number",
                      "minimum": 60
                    }
                  }
                }
              }
            }
          },
          "429": {
            "description": "Слишком много запросов",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ThrottledErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/auth/refresh": {
      "get": {
        "operationId": "refresh",
        "tags": ["auth"],
        "summary": "Обновление токенов авторизации",
        "description": "Обновляет токены по refresh-cookie или заголовку.",
        "parameters": [
          {
            "name": "x-refresh-token",
            "in": "header",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Токены обновлены",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TokenPair"
                }
              }
            }
          },
          "401": {
            "description": "Невалидный refresh-токен",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/auth/complete-profile": {
      "post": {
        "operationId": "completeProfile",
        "tags": ["auth", "profile"],
        "summary": "Дополнение профиля пользователя",
        "description": "Обновляет профиль после первичного входа.",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["registrationSessionId"],
                "additionalProperties": false,
                "properties": {
                  "registrationSessionId": {
                    "type": "string",
                    "minLength": 1,
                    "description": "Идентификатор сессии регистрации из verifyOtp"
                  },
                  "fullName": {
                    "type": "string",
                    "minLength": 1,
                    "maxLength": 100
                  },
                  "avatarMedia": {
                    "$ref": "#/components/schemas/LinkMediaData"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Профиль создан, токены выданы",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "required": ["user", "accessToken", "refreshToken"],
                  "additionalProperties": false,
                  "properties": {
                    "user": {
                      "$ref": "#/components/schemas/User"
                    },
                    "accessToken": {
                      "type": "string",
                      "minLength": 16,
                      "description": "JWT access-токен"
                    },
                    "refreshToken": {
                      "type": "string",
                      "minLength": 16,
                      "description": "JWT refresh-токен"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Некорректный запрос",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/auth/logout": {
      "post": {
        "operationId": "logout",
        "tags": ["auth"],
        "summary": "Выход из аккаунта",
        "description": "Удаляет текущую сессию пользователя.",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "204": {
            "description": "Сессия удалена"
          },
          "401": {
            "description": "Не авторизован",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/me": {
      "get": {
        "operationId": "getMe",
        "tags": ["auth", "profile"],
        "summary": "Получение текущего пользователя",
        "description": "Возвращает профиль текущего пользователя.",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Профиль пользователя",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/User"
                }
              }
            }
          },
          "401": {
            "description": "Не авторизован",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/me/profile": {
      "patch": {
        "operationId": "updateProfile",
        "tags": ["auth", "profile"],
        "summary": "Обновление профиля пользователя",
        "description": "Обновляет профиль текущего пользователя.",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["fullName"],
                "additionalProperties": false,
                "properties": {
                  "fullName": {
                    "type": "string",
                    "minLength": 2,
                    "maxLength": 100,
                    "description": "Полное имя пользователя"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Обновлённый профиль",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/User"
                }
              }
            }
          },
          "400": {
            "description": "Невалидное имя",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "401": {
            "description": "Не авторизован",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/me/sessions": {
      "get": {
        "operationId": "getMeSessions",
        "tags": ["auth", "sessions"],
        "summary": "Получение списка активных сессий",
        "description": "Возвращает список активных сессий текущего пользователя.",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Список сессий",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "required": ["sessions"],
                  "additionalProperties": false,
                  "properties": {
                    "sessions": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/UserSession"
                      }
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Не авторизован",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      },
      "delete": {
        "operationId": "deleteAllSessions",
        "tags": ["auth", "sessions"],
        "summary": "Выход со всех устройств",
        "description": "Удаляет все сессии текущего пользователя, кроме текущей.",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "204": {
            "description": "Все сессии удалены"
          },
          "401": {
            "description": "Не авторизован",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/me/sessions/{sessionId}": {
      "delete": {
        "operationId": "deleteSession",
        "tags": ["auth", "sessions"],
        "summary": "Удаление сессии",
        "description": "Удаляет указанную сессию текущего пользователя.",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "name": "sessionId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "204": {
            "description": "Сессия удалена"
          },
          "401": {
            "description": "Не авторизован",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "404": {
            "description": "Сессия не найдена",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
